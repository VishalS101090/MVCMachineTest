@model Category
@{ 
    ViewData["Title"] = "Details";
}

<div class="mt-5 row">
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => m.CategoryID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => m.CategoryID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => m.CategoryName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => m.CategoryName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(m => m.CreationTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(m => m.CreationTime)
        </dd>
    </dl>

    <a class="mt-2" asp-action="Index">Go Back</a>
</div>
